{
  "help": {
    "help": "Show this help menu !",
    "usage": "help [optional_string]",
    "premium": false,
    "sub_commands": false
  },
  "pause": {
    "help": "Set the player to pause !",
    "usage": "pause",
    "premium": true,
    "sub_commands": false
  },
  "resume": {
    "help": "Set the player to play after a pause !",
    "usage": "resume",
    "premium": true,
    "sub_commands": false
  },
  "queue": {
    "help": "Show the current queue !",
    "usage": "queue",
    "premium": null,
    "sub_commands": false
  },
  "now": {
    "help": "Show what currently playing !",
    "usage": "now",
    "premium": false,
    "sub_commands": false
  },
  "favorite": {
    "help": "Add (if not added) or delete (if even added) the current song from one of your playlist !",
    "usage": "favorite <list_name>",
    "premium": null,
    "sub_commands": false
  },
  "userinfo": {
    "help": "Show informations about current user !",
    "usage": "userinfo",
    "premium": false,
    "sub_commands": false
  },
  "skip": {
    "help": "Skip the current track to the next track of the queue...",
    "usage": "skip",
    "premium": null,
    "sub_commands": false
  },
  "previous": {
    "help": "Skip the current track to the previous track...",
    "usage": "previous",
    "premium": null,
    "sub_commands": false
  },
  "volume": {
    "help": "Change the volume of the player !",
    "usage": "volume <percent>",
    "premium": true,
    "sub_commands": false
  },
  "playlist": {
    "help": "Actions on user's playlists !",
    "usage": "playlist <sub-command> [string]",
    "premium": null,
    "sub_commands": true,
    "subcommands": {
      "list": {
        "help": "List all playlist linked to this account !",
        "usage": "playlist list",
        "premium": false
      },
      "create": {
        "help": "Create a playlist for this account !",
        "usage": "playlist create [string_name]",
        "premium": null
      },
      "delete": {
        "help": "Delete a playlist from this account !",
        "usage": "playlist delete [string_name]",
        "premium": null
      }
    }
  }
}